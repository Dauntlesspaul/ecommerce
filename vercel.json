{
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    },
    {
      "src": "aserver/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/aserver/(.*)", "dest": "/aserver/server.js" },
    { "src": "/(.*)", "dest": "/client/build/$1" }
  ]
}